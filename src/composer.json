{
    "name": "blog/simple-blog",
    "description": "Simple PHP blog with Smarty template engine",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "smarty/smarty": "^4.3",
        "ext-pdo": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "App\\": "App/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
